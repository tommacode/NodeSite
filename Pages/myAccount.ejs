<!DOCTYPE html>
<html lang="en">
  <head>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Poppins:wght@200;400;600&display=swap"
      rel="stylesheet"
    />
    <script src="/login.js"></script>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Tom</title>
    <!--Meta Tags-->
    <meta name="twitter:card" content="summary" />
    <meta name="twitter:domain" content="tomblake.me" />
    <meta
      name="twitter:title"
      property="og:title"
      itemprop="name"
      content="Manage your account"
    />
    <meta
      name="twitter:description"
      property="og:description"
      itemprop="description"
      content="Manage your account"
    />
    <link rel="stylesheet" href="css.css" />
    <link rel="icon" type="image/x-icon" href="/favicon.ico" />
  </head>
  <body>
    <div></div>
    <div id="frosted">
      <nav>
        <a href="/login" class="listLink" style="font-size: 1rem" id="loginInfo"
          >Not logged in</a
        >
        <ul>
          <li><a class="listLink" href="/">Home</a></li>
          <li><a class="listLink" href="/projects">Articles</a></li>
        </ul>
      </nav>
      <!--Big header to draw attention-->
      <header>
        <h1>Welcome</h1>
        <h3>Manage your account</h3>
      </header>
      <!--Bio-->
      <div class="borders">
        <h3>Account</h3>
        <p>
          <a class="listLink" href="/Logout">Logout</a>
        </p>
      </div>
      <div class="borders">
        <h3>Sessions</h3>
        <% for (i = 0; i < Sessions.length;i++) { %>
        <div class="borders">
          <h4>Session <%= i+1 %> <%= Sessions[i].CurrentSession %></h4>
          <ul>
            <li>
              <strong>Time Created: </strong>
              <%= Sessions[i].TimeCreated %>
            </li>
            <li>
              <strong>Last Login: </strong><%= Sessions[i].TimeLastUsed %>
            </li>
            <li>
              <strong>IP Created with: </strong><%= Sessions[i].IPCreatedWith %>
            </li>
            <li>
              <strong>IP Last Seen: </strong><%= Sessions[i].IPLastSeen %>
            </li>
            <li>
              <strong>User Agent Created with: </strong><%=
              Sessions[i].UserAgentCreatedWith %>
            </li>
            <li>
              <strong>User Agent Last seen: </strong><%=
              Sessions[i].UserAgentLastSeen %>
            </li>
          </ul>
        </div>
        <% }; %>
      </div>
      <div id="footer">
        <h3>Footer</h3>
        <!--Links-->
        <ul class="linkBar">
          <li>
            <a
              target="_blank"
              href="https://github.com/tommacode"
              class="listLink"
              >My Github</a
            >
          </li>
          <li>
            <a
              target="_blank"
              href="https://tomhasawebsite.com"
              class="listLink"
              >Other Website</a
            >
          </li>
          <li><a href="#" class="listLink">Back to the top!</a></li>
        </ul>
      </div>
    </div>
  </body>
</html>
